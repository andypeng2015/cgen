module popcnt

export function $popcnt8(b %n/rdi) {
@2:
  %0 = popcnt.b %n ; @10
  ret rax/%0
}

export function $popcnt16(h %n/rdi) {
@4:
  %1 = popcnt.h %n ; @11
  ret rax/%1
}

export function $popcnt32(w %n/rdi) {
@6:
  %2 = popcnt.w %n ; @12
  ret rax/%2
}

export function $popcnt64(l %n/rdi) {
@8:
  %3 = popcnt.l %n ; @13
  ret rax/%3
}
