module evenodd

function $abs(w %n/rdi) {
@2:
  %9 = slt.w %n, 0x0_w ; @30
  br %9, @3, @21(%n)
@3:
  %10 = neg.w %n ; @31
  jmp @21(%10)
@21(%0):
  ret rax/%0
}

export function $even(w %n/rdi) {
@7:
  %n.1/w/rax = call $abs(%n/rdi) ; @10
  %11 = eq.w %n.1, 0x0_w ; @32
  br %11, @22(0x1_b), @9
@9:
  %12 = sub.w %n.1, 0x1_w ; @33
  %b.1/b/rax = call $odd(%12/rdi) ; @13
  jmp @22(%b.1)
@22(%1):
  ret rax/%1
}

export function $odd(w %n/rdi) {
@14:
  %n.1/w/rax = call $abs(%n/rdi) ; @17
  %13 = eq.w %n.1, 0x0_w ; @34
  br %13, @23(0x0_b), @16
@16:
  %14 = sub.w %n.1, 0x1_w ; @35
  %b.1/b/rax = call $even(%14/rdi) ; @20
  jmp @23(%b.1)
@23(%2):
  ret rax/%2
}
