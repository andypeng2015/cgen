function $sum(l %s/rdi) {
  %p = slot 8, align 8
@2:
  st.l %s, %p ; @24
  %a.1 = ld.w %p ; @4
  %0.1 = add.l %p, 0x4_l ; @19
  %b.1 = ld.w %0.1 ; @6
  %1.1 = add.w %a.1, %b.1 ; @20
  ret rax/%1.1
}

export function $sumphi(l %a/rdi, l %b/rsi, w %x/rdx) {
  %ra = slot 8, align 8
  %9 = slot 8, align 8
  %11 = slot 8, align 8
@8:
  st.l %a, %ra ; @28
  %2.1 = slt.w %x, 0x0_w ; @21
  br %2.1, @9, @10
@9:
  st.l %a, %11 ; @34
  jmp @14(%11)
@10:
  st.l %b, %9 ; @32
  jmp @14(%9)
@14(%u.1):
  st.w 0x5_w, %ra ; @17
  %13.1 = ld.l %u.1 ; @35
  %s.1/w/rax = call $sum(%13.1/rdi) ; @18
  ret rax/%s.1
}
